mcp:
  servers:
    tavily:
      command: "npx"
      args: ["-y", "tavily-mcp@0.1.3"]
      env: { "TAVILY_API_KEY": "<your api key>" }
    firecrawl:
      command: "npx"
      args: [ "-y", "firecrawl-mcp" ]
      env: { "FIRECRAWL_API_KEY": "<your api key>" }
    python:
      command: "uv"
      args: [ "--directory",
              "<your deer-go dir>/mcps/python",
              "run",
              "server.py",
      ]
model:
  default_model: 
    model_id: "<your reasoning model>"
    base_url: "<your base url>"
    api_key: "<your api key>"

setting:
  max_plan_iterations: 1
  total_max_round: 3
  agent_max_step: 40
  max_limit_token: 50000